generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    String  @id @default(uuid()) @map("_id")  // Usando UUID para gerar automaticamente o ID
  email String  @unique
  nome  String
  idade String
}
